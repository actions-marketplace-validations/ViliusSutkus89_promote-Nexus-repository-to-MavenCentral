{
  "name": "promote-nexus-repository-to-mavencentral",
  "version": "1.1.0",
  "private": false,
  "description": "Promote a staging Nexus repository to MavenCentral",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "echo 'All (none) tests passed'",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViliusSutkus89/promote-Nexus-repository-to-MavenCentral.git"
  },
  "keywords": [
    "Nexus",
    "Sonatype",
    "MavenCentral"
  ],
  "author": "Vilius Sutkus <ViliusSutkus89@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "axios": "^0.21.1",
    "Buffer": "0.0.0",
    "parse-apache-directory-index": "^2.1.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^16.4.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/parser": "^5.47.0",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-prettier": "^3.4.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "typescript": "^4.1.3"
  }
}
